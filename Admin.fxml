<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <Label layoutX="54.0" layoutY="42.0" prefHeight="17.0" prefWidth="43.0" text="Users" />
      <ComboBox fx:id="comboBox" layoutX="54.0" layoutY="59.0" onAction="#comboBoxAction" prefWidth="150.0" />
      <Button fx:id="addUser" layoutX="455.0" layoutY="156.0" mnemonicParsing="false" onAction="#addUserAction" text="Add User" />
      <Button fx:id="deleteUser" layoutX="225.0" layoutY="60.0" mnemonicParsing="false" onAction="#deleteUserAction" text="Delete User" />
      <TextField fx:id="username" layoutX="401.0" layoutY="73.0" onAction="#usernameAction" promptText="username" />
      <TextField fx:id="password" layoutX="401.0" layoutY="113.0" onAction="#passwordAction" promptText="password" />
   </children>
</AnchorPane>
