<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane accessibleRole="PASSWORD_FIELD" prefHeight="350.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="controllers.CreateAccountController">
   <children>
      <Label layoutX="72.0" layoutY="56.0" prefHeight="29.0" prefWidth="63.0" text="Username" />
      <Label layoutX="74.0" layoutY="102.0" prefHeight="29.0" prefWidth="63.0" text="Password" />
      <TextField fx:id="username" layoutX="144.0" layoutY="57.0" />
      <Button fx:id="signUp" layoutX="72.0" layoutY="148.0" mnemonicParsing="false" onAction="#signUpAction" prefHeight="27.0" prefWidth="239.0" text="Sign Up Now" />
      <Label layoutX="131.0" layoutY="210.0" prefHeight="17.0" prefWidth="121.0" text="Already signed up?" />
      <Button fx:id="back" layoutX="160.0" layoutY="233.0" mnemonicParsing="false" onAction="#backAction" prefHeight="17.0" prefWidth="63.0" text="Go Back">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Label fx:id="usernameTaken" layoutX="134.0" layoutY="85.0" prefHeight="17.0" prefWidth="188.0" text="Username already taken, try again." textFill="RED" visible="false">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <TextField fx:id="password" layoutX="144.0" layoutY="103.0" />
   </children>
</AnchorPane>
